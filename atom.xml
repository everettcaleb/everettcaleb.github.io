---
layout: feed
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
	<title>Caleb Everett's Website</title>
	<link href="https://everettcaleb.com/atom.xml" rel="self"/>
	<link href="https://everettcaleb.com"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>https://everettcaleb.com</id>
	<author>
		<name>Caleb Everett</name>
		<email>caleb@everettcaleb.com</email>
	</author>
	{% for post in site.posts %}
		<entry>
			<title>{{ post.title }}</title>
			<link href="https://everettcaleb.com{{ post.url }}"/>
			<updated>{{ post.date | date_to_xmlschema }}</updated>
			<id>https://everettcaleb.com{{ post.id }}</id>
			<content type="html">{{ post.content | xml_escape }}</content>
		</entry>
	{% endfor %}
</feed>
